<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="planbServer.planb.cscenter.repository.NoticeDAO">

        <select id="getNoticeList" resultType="planbServer.planb.cscenter.model.NoticeVO">
        SELECT  notice_id,
                notice_title,
                notice_content
        FROM    notice
        ORDER BY notice_id DESC
        </select>

</mapper>